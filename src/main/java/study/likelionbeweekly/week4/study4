create database example;

show databases;

use example;

create table Users(
    id       varchar(10) primary key,
    Username varchar(10) not null
    );

    //name 활성화 이슈 해결 > drop table if exists Users; >> Users 를 날리겠다!

show tables; // ctrl + enter

desc Users; // 표 확인 가능

insert into Users(id, username)
values('go', 'minseok');
select id, username from Users;
select * from Users; // 모든 필드 가져올 때는 * 사용 100개 하나하나 치는 거 불가능 .. . . .

update Users set username = 'mmmm' where id = 'go';

delete Users